{
  "name": "begintrips",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "_generate-product-plans": "ts-node ./scripts/generatePlans.ts",
    "_generate-types": "supabase gen types --lang=typescript --project-id $SUPABASE_PROJECT_ID --schema public > src/libs/supabase/database.types.ts",
    "build": "next build",
    "postbuild": "next-sitemap",
    "build-storybook": "storybook build",
    "predev": "node -v | grep -q \"$(cat .nvmrc)\" || (echo \"Node version does not match .nvmrc. Please run 'nvm use'\" && exit 1)",
    "dev": "yarn predev && next dev",
    "extract": "formatjs extract './src/{components,pages,utils}/**/*.ts*' --ignore='**/*.d.ts' --out-file ./assets/translations/en.json --format simple",
    "generate:product-plans": "dotenv -- yarn _generate-product-plans",
    "generate:types": "dotenv -- yarn _generate-types",
    "lint": "next lint",
    "start": "next start",
    "storybook": "storybook dev -p 6006 --no-open",
    "stripe:listen": "stripe listen --forward-to=localhost:3000/api/stripe/webhook",
    "stripe:login": "stripe login",
    "test": "jest"
  },
  "resolutions": {
    "ora": "5.4.1"
  },
